  },
  "devDependencies": {
    "concurrently": "1.0.0",
    "karma": "0.12.22",
    "karma-chrome-launcher": "0.1.4",
    "karma-jasmine": "0.1.5",
    "live-server": "0.9.0",
    "typescript": "1.7.3"
